<%= form_for(@project) do |f| %>

  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :author %>
  <%= f.text_field :author %>

  <%= f.label :desc %>
  <%= f.text_field :desc %>

  <%= f.label :image_url %>
  <%= f.text_field :image_url %>

  <%= f.label :github_url %>
  <%= f.text_field :github_url %>

  <%= f.submit %>

<% end %>